                        // XMLï¿½Æ”Fï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ü‚ï¿½ï¿½B
                        response.setContentType(&quot;text/xml&quot;);
                } else if (filename.endsWith(&quot;.html&quot;)) {
