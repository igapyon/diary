    }

    public void process() {
        final Properties props = new Properties();

        // ï¿½ï¿½{ï¿½ï¿½ï¿½Bï¿½ï¿½ï¿½ï¿½ï¿½Å‚ï¿½ gmailï¿½Ö‚ÌÚ‘ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ü‚ï¿½ï¿½B
        props.setProperty(&quot;mail.pop3.host&quot;, &quot;pop.gmail.com&quot;);
        props.setProperty(&quot;mail.pop3.port&quot;, &quot;995&quot;);

        // ï¿½^ï¿½Cï¿½ï¿½ï¿½Aï¿½Eï¿½gï¿½İ’ï¿½
        props.setProperty(&quot;mail.pop3.connectiontimeout&quot;, &quot;60000&quot;);
        props.setProperty(&quot;mail.pop3.timeout&quot;, &quot;60000&quot;);

        // SSLï¿½Ö˜Aï¿½İ’ï¿½
        props.setProperty(&quot;mail.pop3.socketFactory.class&quot;, &quot;javax.net.ssl.SSLSocketFactory&quot;);
        props.setProperty(&quot;mail.pop3.socketFactory.fallback&quot;, &quot;false&quot;);
        props.setProperty(&quot;mail.pop3.socketFactory.port&quot;, &quot;995&quot;);

        final Session session = Session.getInstance(props, new Authenticator() {
            protected PasswordAuthentication getPasswordAuthentication() {
                return new PasswordAuthentication(&quot;ï¿½ï¿½Mï¿½Òï¿½ï¿½ï¿½ï¿½ï¿½@gmail.com&quot;, &quot;password&quot;);
            }
        });

        // ï¿½fï¿½oï¿½bï¿½Oï¿½ï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½Bï¿½Wï¿½ï¿½ï¿½oï¿½Í‚Éƒgï¿½ï¿½ï¿½[ï¿½Xï¿½ï¿½ï¿½oï¿½Ü‚ï¿½ï¿½B
        session.setDebug(true);

        Store store = null;
        try {
            try {
                store = session.getStore(&quot;pop3&quot;);
            } catch (NoSuchProviderException e) {
                System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½wï¿½ï¿½vï¿½ï¿½ï¿½oï¿½Cï¿½_[pop3]ï¿½Ìæ“¾ï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot;
                        + e.toString());
                return;
            }

            try {
                store.connect();
            } catch (AuthenticationFailedException e) {
                System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½Tï¿½[ï¿½oï¿½Ú‘ï¿½ï¿½ï¿½ï¿½É”Fï¿½Ø‚Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot; + e.toString());
                return;
            } catch (MessagingException e) {
                System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½Tï¿½[ï¿½oï¿½Ú‘ï¿½ï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot; + e.toString());
                return;
            }

            Folder folder = null;
            try {
                try {
                    // INBOXï¿½Í—\ï¿½ï¿½ï¿½Å‚ï¿½ï¿½B
                    folder = store.getFolder(&quot;INBOX&quot;);
                } catch (MessagingException e) {
                    System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: INBOXï¿½tï¿½Hï¿½ï¿½ï¿½_ï¿½æ“¾ï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot;
                            + e.toString());
                    return;
                }
                try {
                    folder.open(Folder.READ_ONLY);
                } catch (MessagingException e) {
                    System.out
                            .println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½tï¿½Hï¿½ï¿½ï¿½_ï¿½Iï¿½[ï¿½vï¿½ï¿½ï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot; + e.toString());
                    return;
                }

                // ï¿½ï¿½ï¿½bï¿½Zï¿½[ï¿½Wï¿½ê——ï¿½ï¿½ï¿½æ“¾
                try {
                    final Message messages[] = folder.getMessages();
                    for (int index = 0; index &lt; messages.length; index++) {
                        final Message message = messages[index];

                        // ï¿½ï¿½ï¿½ï¿½APIï¿½ï¿½ï¿½pï¿½ÍˆÍ‚Å‚ï¿½ï¿½ï¿½ï¿½ TOPï¿½Rï¿½}ï¿½ï¿½ï¿½hï¿½~ï¿½Ü‚ï¿½ÅARETRï¿½Rï¿½}ï¿½ï¿½ï¿½hï¿½Í‘ï¿½ï¿½oï¿½ï¿½ï¿½ï¿½È‚ï¿½ï¿½B

                        System.out.println(&quot;Subject: &quot; + message.getSubject());
                        System.out.println(&quot;  Date: &quot; + message.getSentDate().toString());

                        // TODO 0ï¿½Ô–Ú‚Ì”zï¿½ï¿½Aï¿½Nï¿½Zï¿½Xï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½È‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½_ï¿½É’ï¿½ï¿½ÓB
                        final InternetAddress addrFrom = (InternetAddress) message.getFrom()[0];
                        System.out.println(&quot;  From: &quot; + addrFrom.getAddress());
                        // MimeUtility.decodeText(addrFrom.getPersonal())

                        // To: ï¿½ï¿½\ï¿½ï¿½ï¿½B
                        final Address[] addrsTo = message
                                .getRecipients(RecipientType.TO);
                        for (int loop = 0; loop &lt; addrsTo.length; loop++) {
                            final InternetAddress addrTo = (InternetAddress) addrsTo[loop];
                            System.out.println(&quot;  To: &quot; + addrTo.getAddress());
                        }

                        // Cc:ï¿½ÍŠï¿½ï¿½ï¿½

                        // ï¿½È‚ï¿½ï¿½Aï¿½á‚¦ï¿½ï¿½ message.getContentï¿½ï¿½ï¿½Ä‚Ñoï¿½ï¿½ï¿½ï¿½ RETRï¿½Rï¿½}ï¿½ï¿½ï¿½hï¿½ï¿½ï¿½ï¿½ï¿½oï¿½ï¿½ï¿½ï¿½ï¿½B
                    }
                } catch (MessagingException e) {
                    System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½ï¿½ï¿½bï¿½Zï¿½[ï¿½Wï¿½æ“¾ï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot; + e.toString());
                    return;
                }
            } finally {
                if (folder != null) {
                    try {
                        folder.close(false);
                    } catch (MessagingException e) {
                        System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½tï¿½Hï¿½ï¿½ï¿½_ï¿½Nï¿½ï¿½ï¿½[ï¿½Yï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot;
                                + e.toString());
                    }
                }
            }
        } finally {
            if (store != null) {
                try {
                    store.close();
                } catch (MessagingException e) {
                    System.out.println(&quot;ï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½ï¿½M: ï¿½Tï¿½[ï¿½oï¿½Ø’fï¿½Éï¿½ï¿½sï¿½ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½ï¿½B&quot; + e.toString());
                }
            }
        }
    }
}</pre>
